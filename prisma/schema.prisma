generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("SUPABASE_DATABASE_URL")
}

model Profile {
  id String @id

  generatedTitles GeneratedTitle[]

  @@map("profiles")
}

model GeneratedTitle {
  id     String @id @default(cuid())
  user   String
  prompt String
  title  String

  profile Profile @relation(fields: [user], references: [id])

  @@map("generated_titles")
}
